<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             tools:layout_editor_absoluteY="81dp">

    <TextView
            android:id="@+id/textView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="NO:"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="140dp"/>

    <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="173dp"
            android:layout_marginTop="12dp"
            android:text="NAME:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/myImg"
            app:layout_constraintStart_toStartOf="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/textView"/>

    <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="1dp"
            android:layout_marginTop="8dp"
            android:text="AGE:"
            app:layout_constraintStart_toStartOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/textView3"/>

    <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginEnd="4dp"
            android:text="TEL:"
            app:layout_constraintBottom_toBottomOf="@+id/lisTel"
            app:layout_constraintEnd_toEndOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="@+id/lisTel"/>

    <TextView
            android:id="@+id/lisNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="26dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="parent"/>

    <TextView
            android:id="@+id/lisName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="@+id/lisNo"
            app:layout_constraintTop_toBottomOf="@+id/lisNo"/>

    <TextView
            android:id="@+id/lisAge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="@+id/lisName"
            app:layout_constraintTop_toBottomOf="@+id/lisName"/>

    <TextView
            android:id="@+id/lisTel"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginTop="31dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="@+id/lisAge"
            app:layout_constraintTop_toTopOf="@+id/lisAge"/>

    <ImageView
            android:id="@+id/myImg"
            android:layout_width="124dp"
            android:layout_height="131dp"
            android:layout_marginBottom="5dp"
            android:layout_marginEnd="3dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView8"
            app:layout_constraintEnd_toStartOf="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:srcCompat="@drawable/madchen"/>
    <TextView
            android:text="address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView13" android:layout_marginTop="8dp"
            android:layout_marginStart="1dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8" app:layout_constraintStart_toEndOf="@+id/myImg"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/lisAdd"
            app:layout_constraintStart_toStartOf="@+id/lisTel" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/lisTel"/>
</android.support.constraint.ConstraintLayout>